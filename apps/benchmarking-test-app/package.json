{
  "name": "benchmarking-test-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "build:android": "npm run mkdist && react-native bundle --entry-file index.js --platform android --dev true --bundle-output dist/main.android.jsbundle --assets-dest dist/res --sourcemap-output ./dist/sourceMap.js",
    "build:ios": "npm run mkdist && react-native bundle --entry-file index.js --platform ios --dev true --bundle-output dist/main.ios.jsbundle --assets-dest dist",
    "build:ios:release": "npm run mkdist && react-native bundle --entry-file index.js --platform ios --dev false --bundle-output dist/main.ios.jsbundle --assets-dest dist",
    "ios": "react-native run-ios",
    "lint": "eslint src/ App.tsx",
    "mkdist": "node -e \"require('node:fs').mkdirSync('dist', { recursive: true, mode: 0o755 })\"",
    "start": "react-native start",
    "test": "jest",
    "install:ios:hermes": "(export USE_HERMES=1; pod install --project-directory=ios)",
    "install:ios:jsc": "(export USE_HERMES=0; pod install --project-directory=ios)"
  },
  "dependencies": {
    "@callstack/react-native-visionos": "^0.75.0",
    "@ir-engine/ecs": "^1.6.0",
    "@ir-engine/hyperflux": "^1.6.0",
    "bitecs": "^0.3.40",
    "events": "^3.3.0",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "react": "18.2.0",
    "react-native": "^0.75.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-permissions": "^3.10.1",
    "react-native-release-profiler": "^0.2.1",
    "three": "^0.167.1",
    "ts-matches": "^5.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.9.1",
    "@react-native/babel-preset": "^0.75.0",
    "@react-native/eslint-config": "^0.75.0",
    "@react-native/metro-config": "^0.75.0",
    "@react-native/typescript-config": "^0.75.0",
    "@rnx-kit/metro-config": "^1.3.15",
    "@types/react": "^18.2.6",
    "@types/react-native-get-random-values": "^1.8.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/three": "^0.167.2",
    "babel-jest": "^29.6.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-test-app": "^3.9.2",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "typescript-eslint": "^8.3.0"
  },
  "engines": {
    "node": ">=18"
  }
}
